{
  "name": "deso-frontend",
  "version": "0.0.0",
  "config": {
    "deploymentSourceBucketLocation": "s3://bc-roadmap-web-stack-bucketcodebuildsource-1latp2euby59d",
    "profile": "bitcloutdev",
    "region": "us-west-2"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "npm run ng-high-memory && ng build --configuration=production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng-high-memory": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng",
    "pull-desoscript": "npm update --save @adonoustech/desoscript-core @adonoustech/desoscript-extras @adonoustech/desoscript-aws @adonoustech/desoscript-api",
    "install-desoscript": "npm install --save @adonoustech/desoscript-core @adonoustech/desoscript-extras @adonoustech/desoscript-aws @adonoustech/desoscript-api",
    "deploy": "mv ./buildspecs/buildspec.yml ./ && npm run private-package-build-source && npm run private-push-build-to-pipeline-source && mv ./buildspec.yml ./buildspecs/",
    "private-package-build-source": "rm build-source.zip && zip -q -r build-source.zip ./src buildspec.yml package.json angular.json tsconfig.json tsconfig.app.json tslint.json tailwind.config.js",
    "private-push-build-to-pipeline-source": "aws s3 cp ./build-source.zip %npm_package_config_deploymentSourceBucketLocation% --region %npm_package_config_region% --profile %npm_package_config_profile%"
  },
  "browserslist": [
    "> 5%"
  ],
  "private": true,
  "dependencies": {
    "@adonoustech/desoscript-aws": "^0.17.0",
    "@adonoustech/desoscript-core": "^0.17.4",
