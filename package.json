    "@aws-amplify/core": "^4.2.2",
    "@aws-sdk/client-dynamodb": "^3.27.0",
    "@aws-sdk/client-lambda": "^3.27.0",
    "amazon-cognito-identity-js": "^5.1.0",
    "amplitude-js": "^7.4.3",
